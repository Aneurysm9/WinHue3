<Window x:Class="WinHue3.Functions.RoomMap.Form_RoomMap"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinHue3.Functions.RoomMap"
        mc:Ignorable="d"
        Title="Form_RoomMap" Height="450" Width="800">
    <Window.DataContext>
        <local:RoomMapViewModel/>
    </Window.DataContext>
    <Grid>

        <Image Margin="157,0,0.333,-0.333"/>
        <ListBox HorizontalAlignment="Left" Width="157" Margin="0,0,0,39.667"/>
        <Button Content="Select Image..." HorizontalAlignment="Left" Margin="68,391,0,0" VerticalAlignment="Top" Width="89" Command="{Binding ChooseImageCommand}"/>

    </Grid>
</Window>
